{
	"d-dropdown": {
		"prefix": "d-dropdown",
		"body": [
            "<section>",
            "  <div class=\"btn-group g-dropdown\" dDropDown (onToggle)=\"onToggle($event)\">",
            "    <a dDropDownToggle class=\"devui-dropdown-default\">",
            "      更多操作",
            "      <span class=\"devui-caret\"></span>",
            "    </a>",
            "",
            "    <ul dDropDownMenu class=\"dropdown-menu devui-dropdown-menu\" role=\"menu\">",
            "      <li role=\"menuitem\">",
            "        <a class=\"dropdown-item\">(${1:菜单一})</a>",
            "      </li>",
            "      <li class=\"disabled\" role=\"menuitem\">",
            "        <a class=\"dropdown-item disabled\">(${2:菜单二})</a>",
            "      </li>",
            "      <li role=\"menuitem\">",
            "        <a class=\"dropdown-item\">(${3:菜单三})</a>",
            "      </li>",
            "      <li role=\"menuitem\">",
            "        <a class=\"dropdown-item\">(${4:菜单四})</a>",
            "      </li>",
            "    </ul>",
            "  </div>",
            "</section>"
		],
		"description": "dDropDown"
	} 
}