{
	"name": "autodevui",
	"displayName": "AutoDevui",
	"description": "Devui的配套编辑插件",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld",
		"onCommand:extension.showError",
		"onCommand:extension.getCurrentFilePath"
	],
	"main": "src/extension.ts",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "extension.showError",
				"title": "ShowError"
			},
			{
				"command": "extension.getCurrentFilePath",
				"title":"The Source?"
			}
		],
		"menus": {
			"editor/context": [
                {
                    "when": "editorFocus",
                    "command": "extension.getCurrentFilePath",
                    "group": "navigation@5"
                }
            ],
            "explorer/context": [
                {
                    "command": "extension.getCurrentFilePath",
                    "group": "navigation@5"
                }
            ]
        }
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	}
}
